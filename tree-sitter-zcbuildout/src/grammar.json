{
  "name": "zcbuildout",
  "rules": {
    "profile": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[\\n]*"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "comment"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "section"
              }
            ]
          }
        }
      ]
    },
    "section": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_section_header"
          },
          {
            "type": "PATTERN",
            "value": "[\\n]+"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "option"
                },
                {
                  "type": "SYMBOL",
                  "name": "comment"
                }
              ]
            }
          }
        ]
      }
    },
    "_section_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "section_name",
          "content": {
            "type": "SYMBOL",
            "name": "section_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": ":"
                },
                {
                  "type": "FIELD",
                  "name": "section_condition",
                  "content": {
                    "type": "SYMBOL",
                    "name": "section_condition"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "section_name": {
      "type": "PATTERN",
      "value": "[^\\[\\]:\\n]+"
    },
    "section_condition": {
      "type": "PATTERN",
      "value": "[^\\[\\]\\n]+"
    },
    "option": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "option_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "PATTERN",
          "value": "\\s*"
        },
        {
          "type": "SYMBOL",
          "name": "option_value"
        }
      ]
    },
    "option_name": {
      "type": "PATTERN",
      "value": "[^#;=\\s\\[]+"
    },
    "option_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_option_value_mono_line"
            },
            {
              "type": "PREC_LEFT",
              "value": 1,
              "content": {
                "type": "REPEAT",
                "content": {
                  "type": "SYMBOL",
                  "name": "_option_value_multi_line"
                }
              }
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\n"
            },
            {
              "type": "PREC_LEFT",
              "value": 2,
              "content": {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "_option_value_multi_line"
                }
              }
            }
          ]
        }
      ]
    },
    "_option_value_mono_line": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_option_text"
          },
          {
            "type": "PATTERN",
            "value": "\\n+"
          }
        ]
      }
    },
    "_option_value_multi_line": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\s+"
            },
            {
              "type": "SYMBOL",
              "name": "_option_text"
            },
            {
              "type": "PATTERN",
              "value": "\\n+"
            }
          ]
        }
      }
    },
    "_option_text": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "option_text"
          },
          {
            "type": "SYMBOL",
            "name": "option_with_reference"
          }
        ]
      }
    },
    "option_text": {
      "type": "PATTERN",
      "value": "([^\\n]+\\$\\$[^\\n]*|[^\\n]*\\$\\$[^\\n]+|[^\\$\\n]+)+"
    },
    "option_with_reference": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "${"
        },
        {
          "type": "FIELD",
          "name": "referenced_section",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "referenced_section"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "referenced_option",
          "content": {
            "type": "SYMBOL",
            "name": "referenced_option"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "referenced_section": {
      "type": "PATTERN",
      "value": "[^:\\}\\n]+"
    },
    "referenced_option": {
      "type": "PATTERN",
      "value": "[^\\n\\}]+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -10,
        "content": {
          "type": "PATTERN",
          "value": "[#;]+.*\\n+"
        }
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]+\\n*"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

